{
  "name": "asset-webpack",
  "version": "6.0.0",
  "description": "asset-bundle integration for WebPack",
  "main": "./lib",
  "browser": "./lib",
  "module": "./index",
  "react-native": "./index",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublish": "npm run build",
    "build": "babel ./index.js ./loader.js -d ./lib",
    "lint": "eslint-godaddy *.js ./test/*.js"
  },
  "homepage": "https://github.com/godaddy/asset-system/tree/master/packages/webpack",
  "repository": {
    "type": "git",
    "url": "git@github.com:godaddy/asset-system.git"
  },
  "author": "GoDaddy.com Operating Company, LLC",
  "contributors": [
    "Arnout Kazemier <akazemier@godaddy.com>"
  ],
  "license": "MIT",
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "nyc": {
    "exclude": [
      "lib"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "asset-test": "^1.0.0",
    "eslint": "^4.14.0",
    "eslint-config-godaddy": "^2.1.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^24.7.1",
    "puppeteer": "^1.9.0",
    "webpack": "^4.20.2"
  },
  "dependencies": {
    "asset-bundle": "^4.1.0",
    "async": "^2.6.0",
    "loader-utils": "^1.1.0"
  }
}
